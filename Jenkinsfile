pipeline {
    agent any
    stages {
        stage ('Start'){
            steps{
                echo "This is the first Stage of the Pipeline"
            }
        }
        stage ('Middle') {
            steps{
                echo "This is the Middle Stage of the Pipeline"
            }
        }
        stage ('End') {
            steps{
                echo "This is the End Stage of the Pipeline"
            }
        }
    }
}